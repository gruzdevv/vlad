<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.StackPane?>

<StackPane fx:id="parentContainer" style="-fx-background-color: BLACK;" stylesheets="@../css/main.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MenuController">
    <children>
        <AnchorPane id="AnchorPane" prefHeight="106.0" prefWidth="390.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
            <children>
                <GridPane prefHeight="129.0" prefWidth="390.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="211.0" minWidth="10.0" prefWidth="201.0" />
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="190.0" minWidth="10.0" prefWidth="189.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                    <children>
                        <Button fx:id="nextStreetButton" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="62.0" prefWidth="220.0" text="Улицы" textFill="WHITE" GridPane.columnIndex="1" />
                        <Button fx:id="nextRaionButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="62.0" prefWidth="256.0" text="Районы" textFill="WHITE" GridPane.rowIndex="1" />
                        <Button fx:id="nextTypenedButton" layoutX="110.0" layoutY="10.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="62.0" prefWidth="242.0" text="Типы" textFill="WHITE" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  
                        <Button fx:id="nextNedvizhButton" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="62.0" prefWidth="203.0" text="Недвижимость" textFill="WHITE" />
                    </children>
                </GridPane>
            </children>
        </AnchorPane>
    </children>
</StackPane>
